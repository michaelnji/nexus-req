{
  "name": "nexus-req",
  "version": "2.0.0",
  "author": {
    "name": "COD3V1L",
    "email": "codebase65@outlook.com",
    "url": "https://michaelnji.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelnji/nexus-req.git"
  },
  "main": "./dist/nexus-req.cjs",
  "module": "./dist/nexus-req.esm.js",
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.58",
    "@vitest/coverage-istanbul": "^3.2.2",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^3.2.2",
    "dts-bundle-generator": "^9.5.1",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.5.2",
    "np": "^10.2.0",
    "prettier": "^3.5.3",
    "release-it": "^19.0.3",
    "terser": "^5.41.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "upgradeps": "^2.0.8",
    "vite": "^5.4.19",
    "vitest": "^1.6.1"
  },
  "exports": {
    ".": {
      "require": "./dist/nexus-req.cjs",
      "import": "./dist/nexus-req.esm.js",
      "types": "./dist/nexus-req.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/",
      "types": "./dist/"
    }
  },
  "bugs": {
    "url": "https://github.com/michaelnji/nexus-req/issues"
  },
  "description": "request handling in nuxt simplified",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md",
    "src",
    "package.json"
  ],
  "homepage": "https://github.com/michaelnji/nexus-req#readme",
  "jsdelivr": "./dist/nexus-req.iife.js",
  "keywords": [
    "refid",
    "unique"
  ],
  "license": "MIT",
  "packageManager": "npm@10.8.0",
  "private": false,
  "scripts": {
    "dev": "vite --host",
    "prebuild": "rm -rf dist/*",
    "build": "tsc && vite build",
    "postbuild": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:c": "vitest --coverage",
    "format": "prettier . --write",
    "upgrade": "upgradeps",
    "release": "bun run build && release-it"
  },
  "type": "module",
  "types": "./dist/nexus-req.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/nexus-req.d.ts"
      ]
    }
  },
  "typings": "./dist/nexus-req.d.ts"
}
